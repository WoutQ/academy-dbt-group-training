{
  "name": "dbt Group Training - gcloud",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "root",
  "forwardPorts": [8080, 5000],
  "postCreateCommand": "bash scripts/install_dependencies.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "innoverio.vscode-dbt-power-user"
      ],
      "files.associations": {
        "*.yaml": "jinja-yaml",
        "*.yml": "jinja-yaml",
        "*.sql": "jinja-sql",
        "*.md": "jinja-md"
      }
    }
  }
}
